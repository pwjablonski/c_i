<div class="row">
         <%= render 'layouts/sidebar' %>
        
      <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">

        <div class="container">
          <div class="row">
            <h3>Users</h3>
            <div class="column">
              <table class="table">
                <tbody>
                  <% @users.each do |user| %>
                    <tr>
                      <%= render user %>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
    </div>
</div>