  <div class="container-fluid">
      <div class="row">
         <%= render 'layouts/sidebar' %>
        
      <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
          <p id="notice"><%= notice %></p>
          
          <h1>Listing Users</h1>
          
          <table>
            <thead>
              <tr>
                <th>Name</th>
                <th>Email</th>
                <th colspan="3"></th>
              </tr>
            </thead>
          
            <tbody>
              <% @users.each do |user| %>
                <tr>
                  <td><%= user.name %></td>
                  <td><%= user.email %></td>
                  
                </tr>
              <% end %>
            </tbody>
          </table>
          
          <br>
          
          <%= link_to 'New User', new_user_path %>
       </div>
    </div>
</div>