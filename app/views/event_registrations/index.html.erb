<p id="notice"><%= notice %></p>

<h1>Listing Event Registrations</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Eb event</th>
      <th>Eb attendee</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_registrations.each do |event_registration| %>
      <tr>
        <td><%= event_registration.event_id %></td>
        <td><%= event_registration.eb_event_id %></td>
        <td><%= event_registration.eb_attendee_id %></td>
        <td><%= event_registration.student_id %></td>
        <td><%= link_to 'Show', event_registration %></td>
        <td><%= link_to 'Edit', edit_event_registration_path(event_registration) %></td>
        <td><%= link_to 'Destroy', event_registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event registration', new_event_registration_path %>
